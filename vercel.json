{
    "version": 2,
    "builds": [
      { "src": "frontend/package.json", "use": "@vercel/static-build" },
      { "src": "backend/package.json", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/frontend/(.*)", "dest": "frontend/$1" },
      { "src": "/backend/(.*)", "dest": "backend/$1" }
    ],
    "env": {
      "ENV_VAR": "value"
    },
    "functions": {
      "api/index.js": {
        "runtime": "nodejs12.x",
        "memory": 512,
        "maxDuration": 10
      }
    },
    "alias": { "example": "angular9-essencial" }
  }
  